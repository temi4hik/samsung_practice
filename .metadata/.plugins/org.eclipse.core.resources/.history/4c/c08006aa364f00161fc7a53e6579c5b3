var documentsDir;

function writeLogEnter() {	
	tizen.filesystem.resolve(
		     'documents',
		     function(dir) {
		       documentsDir = dir; 
		       dir.listFiles(function(files) {
		    	   var logFile;
		    	   for (var i = 0; i < files.length; ++i) {
		    		   if (files[i].name == "log.txt") {
		    			   logFile = files[i];
		    			   break;
		    		   }
		    	   }
		    	   
		    	   if (logFile != null) {
		    		   
		    	   } else {
		    		   
		    	   }
		    	   
		    	   
		       },null);
		     }, function(e) {
		       console.log("Error" + e.message);
		     }, "rw"
		 ); 
}
